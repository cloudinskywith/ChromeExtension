{
  "manifest_version":2,
  "name":"天气预报",
  "version":"1.0",
  "description":"查看未来两周天气情况",
  "icons":{
    "16":"images/16X16.png",
    "48":"images/48X48.png",
    "128":"images/128X128.png"
  },
  "browser_action":{
    "default_icon":"images/48X48.png",
    "default_title":"天气预报",
    "default_popup":"popup.html"
  },
  "options_page":"options.html",
  "permissions":[
    "http://api.openweathermap.org/data/2.5/forecast?q=*"
  ]
}